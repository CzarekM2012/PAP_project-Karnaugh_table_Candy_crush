<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox stylesheets="@menu.css" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="karnaugh.MenuController">
   <children>
      <Label fx:id="gameTitle" text="Bekarnaughed" />
      <Button fx:id="startButton" text="Start Game" onAction="#startClicked"/>
      <Button fx:id="highscoreButton" text="Highscores" onAction="#highscoreClicked"/>
      <Button fx:id="quitButton" contentDisplay="LEFT" text="Quit" onAction="#quitClicked"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   
</VBox>
